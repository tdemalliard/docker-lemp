server {
    listen 80;
        server_name localhost.net;
        location / {
        root /srv;
    }

    location ~ \.php$ {
    try_files $uri =404;
    fastcgi_pass   127.0.0.1:9000;
    fastcgi_index  index.php;
    fastcgi_param SCRIPT_FILENAME /home/foo/www$fastcgi_script_name;
    include fastcgi_params;
    }

}